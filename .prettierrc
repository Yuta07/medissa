{
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "all",
  "useTabs": true,
  "printWidth": 120,
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "useTabs": false,
        "trailingComma": "none"
      }
    },
    {
      "files": "*.json",
      "options": {
        "useTabs": false,
        "tabWidth": 2
      }
    }
  ]
}
